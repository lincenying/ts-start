{
  "name": "__ts-start__",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.28.0",
  "description": "LinCenYing's typescript start",
  "author": "LinCenYing <lincenying@gmail.com> (https://github.com/lincenying/)",
  "license": "MIT",
  "homepage": "https://github.com/lincenying/ts-start",
  "keywords": [
    "ts-start"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild -stub",
    "postpublish": "simple-open-url https://github.com/lincenying/ts-start",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish -r --access public",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "tsc --noEmit",
    "prepare": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/ni": "^28.1.0",
    "@lincy/eslint-config": "^6.3.3",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.0",
    "esno": "^4.8.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "simple-git-hooks": "^2.13.1",
    "simple-open-url": "^3.0.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.17"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
